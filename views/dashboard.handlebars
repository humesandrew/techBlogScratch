<div class = "posts">
    {{#each posts}}
    <article class = "card-body">
        <div class = "card-header flex-row justify-space-between">
            <h2>
                {{post.title}}
            </h2>
        <div>
                class="card-text"{{post.body}}
            </div>
        <div>
                Posted By: {{post.name}}
            </div>
        </div>
        <div class = "card-body">
            {{body}}
        </div>
    </article>
    {{/each}}
</div>
<script src="/js/post.js"></script>